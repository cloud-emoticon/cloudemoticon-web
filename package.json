{
  "name": "cloudemoticon-web",
  "description": "Your favorite emoticons in the cloud",
  "author": "KTachibanaM",
  "main": "public/electron.js",
  "version": "1.4.0",
  "license": "MIT",
  "homepage": "./",
  "dependencies": {
    "@material-ui/core": "^4.9.1",
    "@material-ui/icons": "^4.9.1",
    "copy-to-clipboard": "^3.2.1",
    "electron-is-dev": "^2.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "cp-cli": "^2.0.0",
    "cross-env": "^7.0.3",
    "electron": "^25.1.0",
    "electron-builder": "^23.6.0",
    "react-scripts": "^2.1.1"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "react-scripts start",
    "headless": "cross-env BROWSER=none react-scripts start",
    "electron": "electron .",
    "start-electron": "concurrently \"npm run headless\" \"npm run electron\"",
    "build": "react-scripts build",
    "prepackage-electron": "npm run build",
    "package-electron": "electron-builder build",
    "eject": "react-scripts eject"
  },
  "build": {
    "appId": "moe.emoticon.cloudemoticon",
    "productName": "Cloud Emoticon",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories":{
      "buildResources": "assets"
    },
    "win": {
      "icon": "./assets/icon.png"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
