{
  "name": "cloudemoticon-web",
  "productName": "Cloud Emoticon",
  "main": "electron/main.js",
  "version": "1.6.1",
  "license": "MIT",
  "homepage": "./",
  "dependencies": {
    "@material-ui/core": "^4.9.1",
    "@material-ui/icons": "^4.9.1",
    "copy-to-clipboard": "^3.2.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "cp-cli": "^2.0.0",
    "cross-env": "^7.0.3",
    "electron": "^25.1.0",
    "electron-builder": "^23.6.0",
    "react-scripts": "^2.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "headless": "cross-env BROWSER=none react-scripts start",
    "electron": "cross-env ELECTRON_START_URL=http://localhost:3000 electron .",
    "start-electron": "concurrently \"npm run headless\" \"npm run electron\"",
    "build": "react-scripts build",
    "build-electron": "cp-cli electron/. build/electron",
    "package-electron": "electron-builder build -c.extraMetadata.main=build/electron/main.js --publish never",
    "eject": "react-scripts eject"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "github",
      "repo": "electron-cra-example",
      "owner": "johndyer24"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
